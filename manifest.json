{
  "manifest_version": 3,
  "name": "hiREZZIE",
  "version": "3.0.1",
  "description": "hiREZZIE finds the newest and greatest Hi-Res images across the internet.",
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "permissions": [
    "storage",
    "activeTab"
  ],
  "host_permissions": [
    "https://www.googleapis.com/*",
    "<all_urls>"
  ],
  "host_permissions": [
    "<all_urls>"
  ],
"background": {
  "service_worker": "background.js",
  "type": "module"
},
  "action": {
    "default_popup": "popup.html",
    "default_title": "hiREZZIE - Hi-Res Image Finder"
  },
  "options_ui": {
    "page": "options.html",
    "open_in_tab": true
  }
}
